{
  "name": "rikudou/qr-payment-bundle",
  "type": "symfony-bundle",
  "license": "WTFPL",
  "description": "Symfony bundle for generating QR payments for Czech, Slovak and European standards",
  "authors": [
    {
      "name": "Dominik Chrástecký",
      "email": "dominik@chrastecky.cz"
    }
  ],
  "require": {
    "php": ">=7.2",
    "symfony/framework-bundle": ">=4.1",
    "symfony/yaml": ">=4.1",
    "rikudou/czqrpayment": "^4.0",
    "rikudou/skqrpayment": "^2.3",
    "endroid/qr-code": "^3.2",
    "rikudou/installer": "dev-composer_2",
    "rikudou/euqrpayment": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Rikudou\\QrPaymentBundle\\": ""
    }
  }
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/graineeg/RikudouInstaller",
      "minimum-stability": "dev",
      "prefer-stable": true
    }]
}
